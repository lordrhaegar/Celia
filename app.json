{
  "expo": {
    "name": "Celia",
    "slug": "celia",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/celia-app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/celia-app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aimovement.celia"
    },
    "android": {
      "googleServicesFile" : "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/celia-app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vladrhaegar.celia",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "aimovement",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow Celia to access your camera"
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "ad8df38e-5c46-4990-abcb-4a2d770a967f"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ad8df38e-5c46-4990-abcb-4a2d770a967f"
    }
  }
}
